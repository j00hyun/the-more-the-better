background-repeat: no-repeat;\r\n        border-radius: 5px;\r\n    }\r\n\r\n    #email{\r\n        background-size: 20px 20px;\r\n        background-position: 11px 8px;\r\n        background-repeat: no-repeat;\r\n        border-radius: 5px;\r\n    }\r\n\r\n    #comment{\r\n        background-size: 20px 20px;\r\n        background-position: 11px 8px;\r\n        background-repeat: no-repeat;\r\n        border-radius: 5px;\r\n    }\r\n\r\n    textarea {\r\n        width: 100%;\r\n        height: 150px;\r\n        line-height: 150%;\r\n        resize:vertical;\r\n        border-radius: 5px;\r\n    }\r\n\r\n    input:hover, textarea:hover,\r\n    input:focus, textarea:focus {\r\n        background-color:white;\r\n    }\r\n\r\n    #button-blue{\r\n        font-family: 'Montserrat', Arial, Helvetica, sans-serif;\r\n        float:left; /* ÌîåÎ°Ø Ï§ëÏöî(::after Í∞ÄÏÉÅÏöîÏÜå Ïù¥Ïö©)*/\r\n        width: 100%;\r\n        border: #fbfbfb solid 4px;\r\n        cursor:pointer;\r\n        background-color: orange;\r\n        color:white;\r\n        font-size:24px;\r\n        padding-top:22px;\r\n        padding-bottom:22px;\r\n        -webkit-transition: all 0.3s;\r\n        -moz-transition: all 0.3s;\r\n        transition: all 0.3s;\r\n        margin-top:-4px;\r\n        font-weight:700;\r\n\r\n    }\r\n\r\n    #button-blue:hover{\r\n        background-color: rgba(0,0,0,0);\r\n        color: #000000;\r\n    }\r\n\r\n    .ease {\r\n        width: 0;\r\n        height: 74px;\r\n        background-color: #fbfbfb;\r\n        -webkit-transition: .3s ease;\r\n        -moz-transition: .3s ease;\r\n        -o-transition: .3s ease;\r\n        -ms-transition: .3s ease;\r\n        transition: .3s ease;\r\n    }\r\n\r\n    .submit:hover .ease{\r\n        width:100%;\r\n        background-color:white;\r\n    }\r\n\r\n    .cover{\r\n        text-align: center;\r\n    }\r\n\r\n\r\n</style>"]}]}